    ]
  },
  "devDependencies": {
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.19",
    "eslint": "^7.32.0",
    "eslint-config-vtex": "^12.9.5",
    "eslint-config-vtex-react": "^6.9.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@graphql-tools/executor-graphql-ws": "^1.2.0",
    "@graphql-tools/executor-legacy-ws": "^1.1.0",
    "@graphql-tools/url-loader": "^8.0.2",
    "axios": "^1.6.0",
    "graphql": "^16.6.0",
    "graphql-config": "^5.1.2",
    "graphql-yoga": "^5.7.0",
    "next": "^14.2.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.78.0"
  }
